			<item name="delete" xsi:type="string">Cowell\Brand\Block\Adminhtml\Brand\Edit\DeleteButton</item>
			<item name="save" xsi:type="string">Cowell\Brand\Block\Adminhtml\Brand\Edit\SaveButton</item>
			<item name="save_and_continue" xsi:type="string">Cowell\Brand\Block\Adminhtml\Brand\Edit\SaveAndContinueButton</item>
		</item>
	</argument>
	<dataSource name="brand_form_data_source">
		<argument name="dataProvider" xsi:type="configurableObject">
